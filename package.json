{
  "name": "my-monorepo3",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.8",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@1.22.22",
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ],
    "nohoist": [
      "apps/backend/**",
      "apps/backend",
      "apps/backend/react",
      "apps/backend/react-dom",
      "*/react-native",
      "*/@react-native/**",
      "*/expo",
      "*/whatwg-fetch"
    ]
  },
  "resolutions": {
    "react": "19.1.0",
    "react-dom": "19.1.0"
  }
}
